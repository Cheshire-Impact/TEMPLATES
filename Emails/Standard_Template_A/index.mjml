<mjml>
  <mj-head>
    <mj-title>%%subject%%</mj-title>
    <!-- Set A Custom Webfont -->
    <mj-font name="Open Sans" href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i&display=swap" />


    <mj-attributes>

      <!-- Spacing Defaults -->
      <mj-all padding="0px" />

      <mj-section padding-left="10px" padding-right="10px" />
      <mj-column padding="10px" />
      <mj-button padding-top="15px" padding-bottom="15px" />
      <mj-divider padding-top="10px" padding-bottom="10px" />
      <mj-text padding="5px" />
      <mj-navbar-link padding-left="10px" padding-right="10px" />


      <!-- Handle Pardot Regions -->
      <mj-section css-class="pardot-repeat-remove" />
      <mj-navbar-link css-class="pardot-region" />
      <mj-image rel="pardot-region" />
      <mj-button css-class="pardot-removable" />


      <!-- Background Colors -->
      <mj-body background-color="#f6f6f6" />
      <mj-section background-color="#fff" />
      <mj-class name="darkBG" background-color="#092b4c" />


      <!-- Default Styles for All Elements -->
      <mj-all color="#000" font-family="'Open Sans',Arial, Helvetica, sans-serif" font-size="14px" line-height="21px" />

      <!-- Custom Text Colors -->
      <mj-class name="highlightText" color="rgb(255, 115, 0)" />
      <mj-class name="primaryText" color="rgb(9, 43, 76)" />
      <mj-class name="secondaryText" color="rgb(143, 167, 188)" />
      <mj-class name="greyText" color="rgb(144, 144, 144)" />
      <mj-class name="whiteText" color="#fff" />


      <!-- Create Classes For Traditional HTML Elements -->
      <mj-class name="h1" font-size="32px" line-height="40px" font-weight="700" />
      <mj-class name="h2" font-size="26px" line-height="33px" font-weight="700" />
      <mj-class name="h3" font-size="22px" line-height="28px" font-weight="700" />
      <mj-class name="h4" font-size="18px" line-height="23px" font-weight="700" />
      <mj-class name="h5" font-size="13px" line-height="17px" font-weight="400" />
      <mj-class name="h6" font-size="10px" line-height="13px" font-weight="400" />





      <!-- Image Styling *** NOTE: By Default All Images Are Set To Fluid/Responsive And Have A URL Of #, Manually Adjust Static Images and Remove Links With href="" *** -->
      <mj-image align="center" fluid-on-mobile="true" href="#" />

      <!-- Style Intro Text -->
      <mj-class name="introText" font-size="10px" />

      <!-- Style Header Nav -->
      <mj-navbar-link color="rgb(255, 115, 0)" text-transform="uppercase" font-weight="700" font-size="12px" />

      <!-- Button Styles -->
      <mj-button href="#" font-weight="700" text-transform="uppercase" border-radius="2px" rel="pardot-region" background-color="#ff7300" />
      <mj-class name="bigButton" font-size="18px" />

      <!-- Default Divider Style -->
      <mj-divider border-width="1px" border-style="solid" border-color="#e3e3e3" />

      <!-- Other Classes -->
      <mj-class name="readMore" background-color="none" align="left" color="rgb(255, 115, 0)" inner-padding="5px" />


    </mj-attributes>

    <!-- Styles Place In Head -->
    <mj-style>
      a {
        color: #000;
      }

      a:link {
        color: #000;
      }

      a:visited {
        color: #000;
      }

      a.whiteLink {
        color: #fff !important;
        text-decoration: none !important;
      }

      a.greyLink {
        color: #909090 !important;
        text-decoration: none !important;
      }
    </mj-style>



    <!-- Media Queries -->

    <mj-style>
      @media (max-width:480px) {
      td.mobileCenter > div {
      width: 100% !important;
      text-align: center !important;
      }
      }
    </mj-style>
  </mj-head>
